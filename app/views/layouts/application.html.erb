<!DOCTYPE html>
<html>
<head>
  <title>MFMS</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="http://www.w3schools.com/lib/w3.css">

  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
	<link href="https://fonts.googleapis.com/css?family=Abril+Fatface|Baloo+Da|Candal" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>




	
</head>
<body>

	<nav class="navbar navbar-default" >
		<div class="container-fluid">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#myNav" aria-expanded="false">
					<span class="sr-only">Toggle navigation</span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				<%= link_to 'MFMS', root_path, :class => "navbar-brand" %>
			</div> <!-- /.navbar-header -->
			<div class="collapse navbar-collapse" id="myNav">
					<ul class="nav navbar-nav">
						<li><%= link_to 'About', about_path %></li>
						<li><%= link_to 'Services', services_path %></li>
						<li><%= link_to 'Appointments', bookings_path %></li>
						<li><%= link_to 'Contact', contact_path %></li>
						<% if current_user %>
						 	<li><%= link_to current_user.email %></li>
						 	<li><%= link_to "Log Out", logout_path %></li>
						<% else %>
							<li><%= link_to 'Sign Up', signup_path %></li>
							<li><%= link_to 'Log In', login_path %></li>
						<% end %>
					</ul>
			</div>
			

		</div> <!-- /.container-fluid -->
		
	</nav>
	<% flash.each do |message_type, msg| %>
		<div class="<%= flash_class(message_type) %> fade in">
			<%= msg %>
			<%= link_to "x", nil, { class: "close", :data_dismiss => "alert", :aria_label => "close" } %>
		</div>
	<% end %>

	<%= yield %>


